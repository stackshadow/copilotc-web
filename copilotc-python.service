[Unit]
Description=Copilot Web-Client
After=network.target network-online.target nss-lookup.target

[Service]
Type=simple
WorkingDirectory=/var/www/copilotc-web
#PIDFile=/run/nginx-copilotc-web.pid
#PrivateDevices=yes
SyslogLevel=err

ExecStart=/usr/bin/python3 -m http.server 8000
KillSignal=SIGQUIT
KillMode=mixed

[Install]
WantedBy=multi-user.target
